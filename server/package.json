{
  "name": "affordable-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "start": "node ./dist/init.js",
    "create-affordable-database": "yarn --cwd ../database/ init-db",
    "create-affordable-database-docker": "yarn --cwd ../database/ init-db-docker",
    "mock": "yarn --cwd ../database/ mock",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/init.ts",
    "debug": "ts-node-dev --inspect --respawn --transpileOnly ./src/init.ts",
    "test": "yarn test:no-integration",
    "test:no-integration": "jest --detectOpenHandles --forceExit --testPathIgnorePatterns test/integration/*",
    "test:integration": "jest --detectOpenHandles --forceExit --testPathPattern test/integration/*",
    "test:all": "jest --detectOpenHandles --forceExit",
    "generate-docs": "apidoc -i src/routes -o apidoc/",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "affordable-shared-models": "link:../core",
    "apidoc": "^0.20.0",
    "aws-sdk": "^2.552.0",
    "bcrypt": "^4.0.1",
    "bluebird": "^3.7.2",
    "cors": "2.8.4",
    "dotenv": "^8.2.0",
    "email-templates": "^7.1.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.4",
    "image-data-uri": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mock-req-res": "^1.1.1",
    "moment": "2.24.0",
    "mysql": "^2.17.1",
    "nodemailer": "^4.7.0",
    "nodemailer-ses-transport": "^1.5.1",
    "plaid": "^5.1.0",
    "pug": "^3.0.0",
    "qrcode": "^1.3.3",
    "reflect-metadata": "^0.1.13",
    "sinon": "^7.3.2",
    "speakeasy": "^2.0.0",
    "stripe": "^8.60.0",
    "tmp": "^0.1.0",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.1.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mock-req-res": "^1.1.0",
    "@types/mysql": "^2.15.8",
    "@types/node": "^13.13.19",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-ses-transport": "^3.1.5",
    "@types/tmp": "^0.1.0",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "affordable-client": "link:../client",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^24.9.0",
    "jest-html-reporter": "^2.8.0",
    "jest-junit": "^10.0.0",
    "jest-mock-extended": "^1.0.8",
    "morgan": "^1.9.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
